<?xml version='1.0' encoding='utf-8'?>
<widget id="com.webwapsolutions.epizzaappZalinsStenovn" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Zalin's Stenovn</name>
    <description>
        
		Zalin's Stenovn - Restaurant App powered by eRestaurant Suite Pro - leveret af Web Wap Solutions
    </description>
    <author email="support@webwapsolutions.com" href="http://webwapsolutions.com">
        Web Wap Solutions Team - 2017
    </author>
    <dependency id="cordova-plugin-geolocation" />
    <dependency id="cordova-plugin-dialogs" />
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.SplashScreen" />
        <param name="ios-package" value="CDVSplashScreen" />
    </feature>
      <feature name="StatusBar">
            <param name="ios-package" value="CDVStatusBar" onload="true" />
        </feature>
    <platform name="android">
        <preference name="Fullscreen" value="false" />
    </platform>

    <preference name="permissions" value="none" />
    <preference name="phonegap-version" value="cli-7.0.1" />
    <preference name="SplashScreen" value="generic" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="FadeSplashScreen" value="false"/>
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
    <preference name="AndroidLaunchMode" value="singleTask" />
    <preference name="KeepRunning" value="true" />
    <preference name="InAppBrowserStorageEnabled" value="true" />
    <preference name="LoadingDialog" value="Velkommen..." />
    <preference name="LoadingPageDialog" value="Velkommen..." />
    <preference name="LogLevel" value="WARN" />
    <gap:config-file parent="/manifest/application" platform="android">
        <activity android:launchMode="singleInstance" />
    </gap:config-file>
    <plugin name="cordova-plugin-dialogs" spec="~1.3.3" />
    <plugin name="cordova-plugin-statusbar" onload="true" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.3" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.3" />
    <plugin name="cordova-plugin-device" spec="~1.1.6" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.2" />
    
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="sms:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="www/icon.png" />
    
    <!-- android icons -->
<icon src="www/res/screen/android/drawable-ldpi/ic_launcher.png" gap:platform="android" gap:qualifier="ldpi" />
<icon src="www/res/screen/android/drawable-mdpi/ic_launcher.png" gap:platform="android" gap:qualifier="mdpi" />
<icon src="www/res/screen/android/drawable-hdpi/ic_launcher.png" gap:platform="android" gap:qualifier="hdpi" />
<icon src="www/res/screen/android/drawable-xhdpi/ic_launcher.png" gap:platform="android" gap:qualifier="xhdpi" />
<icon src="www/res/screen/android/drawable-xxhdpi/ic_launcher.png" gap:platform="android" gap:qualifier="xxhdpi" />
<icon src="www/res/screen/android/drawable-xxxhdpi/ic_launcher.png" gap:platform="android" gap:qualifier="xxxhdpi" />


<!-- splash -->
<gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="www/res/screen/android/drawable-ldpi/ic_launcher.png" />
<gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="www/res/screen/android/drawable-mdpi/ic_launcher.png" />
<gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="www/res/screen/android/drawable-hdpi/ic_launcher.png" />
<gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="www/res/screen/android/drawable-xhdpi/ic_launcher.png" />
  
<gap:splash gap:platform="ios" height="480" width="320" src="www/res/screen/ios/Default.png" />
<gap:splash gap:platform="ios" height="960" width="640" src="www/res/screen/ios/Default@2x.png" />
<gap:splash gap:platform="ios" height="1136" width="640" src="www/res/screen/ios/Default-568h@2x.png" />
<gap:splash gap:platform="ios" height="1024" width="768" src="www/res/screen/ios/Default-Portrait.png" />
<gap:splash gap:platform="ios" height="1242" width="2208" src="www/res/screen/ios/Default-Portrait-736h@3.png" />
<gap:splash gap:platform="ios" height="1080" width="1920" src="www/res/screen/ios/Default-Portrait-736h@4x.png" />

<gap:splash gap:platform="ios" height="768" width="1024" src="www/res/screen/ios/Default-Landscape.png" />
<gap:splash gap:platform="ios" height="2208" width="1242" src="www/res/screen/ios/Default-Landscape-736h@3x.png" />
<gap:splash gap:platform="ios" height="1920" width="1080" src="www/res/screen/ios/Default-Landscape-736h@4x.png" />


<gap:splash gap:platform="ios" height="1136" width="640" src="www/res/screen/ios/Default-568h@2x.png" />
<gap:splash gap:platform="ios" height="1334" width="750" src="www/res/screen/ios/Default-667h@2x.png" />
<gap:splash gap:platform="ios" height="2048" width="1536" src="www/res/screen/ios/Default-Portrait@2x.png" />
<gap:splash gap:platform="ios" height="1536" width="2048" src="www/res/screen/ios/Default-Landscape@2x.png" />


<platform name="ios">
  <preference name="StatusBarOverlaysWebView" value="false" />
  <preference name="StatusBarBackgroundColor" value="#000000" />
  <preference name="StatusBarStyle" value="blacktranslucent" />
  <preference name="Fullscreen" value="false" />
    
  <splash src="www/res/screen/ios/Default.png" width="320" height="480"/>
  <splash src="www/res/screen/ios/Default@2x.png" width="640" height="960"/>
  <splash src="www/res/screen/ios/Default-Portrait.png" width="768" height="1024"/>
  <splash src="www/res/screen/ios/Default-Portrait@2x.png" width="1536" height="2048"/>
  <splash src="www/res/screen/ios/Default-Landscape.png" width="1024" height="768"/>
  <splash src="www/res/screen/ios/Default-Landscape@2x.png" width="2048" height="1536"/>
  <splash src="www/res/screen/ios/Default-568h@2x.png" width="640" height="1136"/>
  <splash src="www/res/screen/ios/Default-667h@2x.png" width="750" height="1334"/>
  <splash src="www/res/screen/ios/Default-Portrait-736h@3x.png" width="1242" height="2208"/>
  <splash src="www/res/screen/ios/Default-Portrait-736h@4x.png" width="1080" height="1920"/>
  
  <splash src="www/res/screen/ios/Default-Landscape-736h@3x.png" width="2208" height="1242"/>
  <splash src="www/res/screen/ios/Default-Landscape-736h@4x.png" width="1920" height="1080"/>
  
</platform>	

    

<!-- iPhone 7 / 8+ -->
<icon src="www/res/screen/ios/Icon-83.5@2x.png" gap:platform="ios" width="167" height="167" />
<!-- iPhone 6 / 6+ -->
<icon src="www/res/screen/ios/Icon-60@3x.png" gap:platform="ios" width="180" height="180" />
<!-- iPhone / iPod Touch  -->
<icon src="www/res/screen/ios/Icon-60.png" gap:platform="ios" width="60" height="60" />
<icon src="www/res/screen/ios/Icon-60@2x.png" gap:platform="ios" width="120" height="120" />
<!-- iPad -->
<icon src="www/res/screen/ios/Icon-76.png" gap:platform="ios" width="76" height="76" />
<icon src="www/res/screen/ios/Icon-76@2x.png" gap:platform="ios" width="152" height="152" />
<!-- Settings Icon -->
<icon src="www/res/screen/ios/Icon-small.png" gap:platform="ios" width="29" height="29" />
<icon src="www/res/screen/ios/Icon-small@2x.png" gap:platform="ios" width="58" height="58" />
<!-- Spotlight Icon -->
<icon src="www/res/screen/ios/Icon-40.png" gap:platform="ios" width="40" height="40" />
<icon src="www/res/screen/ios/Icon-40@2x.png" gap:platform="ios" width="80" height="80" />
<!-- iPhone / iPod Touch -->
<icon src="www/res/screen/ios/Icon.png" gap:platform="ios" width="57" height="57" />
<icon src="www/res/screen/ios/Icon@2x.png" gap:platform="ios" width="114" height="114" />
<!-- iPad -->
<icon src="www/res/screen/ios/Icon-72.png" gap:platform="ios" width="72" height="72" />
<icon src="www/res/screen/ios/Icon-72@2x.png" gap:platform="ios" width="144" height="144" />
<!-- iPhone Spotlight and Settings Icon -->
<icon src="www/res/screen/ios/Icon-small.png" gap:platform="ios" width="29" height="29" />
<icon src="www/res/screen/ios/Icon-small@2x.png" gap:platform="ios" width="58" height="58" />
<!-- iPad Spotlight and Settings Icon -->
<icon src="www/res/screen/ios/Icon-Small-50.png" gap:platform="ios" width="50" height="50" />
<icon src="www/res/screen/ios/Icon-Small-50@2x.png" gap:platform="ios" width="100" height="100" />
</widget>